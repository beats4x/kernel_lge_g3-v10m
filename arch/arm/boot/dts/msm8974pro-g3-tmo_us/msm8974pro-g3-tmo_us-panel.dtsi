&mdss_dsi0 {
	status = "ok";
	lge,num-of-dsv-enable-gpio = <
		0 0  // evb1  = 0 ; evb2  = 0
		0 1  // rev_a = 0 ; rev_b = 1
		1 2  // rev_c = 1 ; rev_d = 2
		2 2  // rev_e = 2 ; rev_f = 2
		2 2  // rev_g = 2 ; rev_h = 2
		2 2  // rev_10 = 2; rev_11 = 2
		2    // rev_12 = 2
		>;
};

&mdss_dsi1 {
	status = "ok";
	lge,num-of-dsv-enable-gpio = <
		0 0  // evb1  = 0 ; evb2  = 0
		0 1  // rev_a = 0 ; rev_b = 1
		1 2  // rev_c = 1 ; rev_d = 2
		2 2  // rev_e = 2 ; rev_f = 2
		2 2  // rev_g = 2 ; rev_h = 2
		2 2  // rev_10 = 2; rev_11 = 2
		2     // rev_12 = 2
		>;
};

&soc {
	i2c@f9967000 {
		lm3631@29{
			status = "ok";
			revision = "rev_a";
		};
		lm3697@36{
			status = "ok";
			revision = "rev_b...";
			lcd_bl {
				lge,hvled = <
					12 12  // evb1  = hvled1, hvled2  ; evb2  = hvled1, hvled2
					12 23  // rev_a = hvled1, hvled2 ; rev_b = hvled2, hvled3
					23 12  // rev_c = hvled1, hvled2 ; rev_d = hvled1, hvled2
					12 12  // rev_e = hvled1, hvled2 ; rev_f = hvled1, hvled2
					12 12  // rev_g = hvled1, hvled2 ; rev_h = hvled1, hvled2
					12 12  // rev_10 = hvled1, hvled2 ; rev_11 = hvled1, hvled2
					12     // rev_12 = hvled1, hvled2
					>;
			};
		};
		tps65132@3e {
			status = "ok";
			revision = "rev_b...";
		};
     };
};

